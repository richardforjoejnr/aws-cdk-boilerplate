{
  "name": "@aws-boilerplate/functions",
  "version": "1.0.0",
  "description": "Lambda functions for AWS boilerplate",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "for dir in dist/jira-*/; do echo '{\"type\":\"module\"}' > \"${dir}package.json\"; done",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@aws-sdk/client-cost-explorer": "^3.496.0",
    "@aws-sdk/client-dynamodb": "^3.496.0",
    "@aws-sdk/client-s3": "^3.496.0",
    "@aws-sdk/client-sfn": "^3.496.0",
    "@aws-sdk/lib-dynamodb": "^3.496.0",
    "@aws-sdk/s3-request-presigner": "^3.496.0",
    "csv-parse": "^5.5.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.958.0",
    "@smithy/util-stream": "^4.5.8",
    "@types/aws-lambda": "^8.10.132",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.0",
    "@types/uuid": "^9.0.7",
    "aws-sdk-client-mock": "^4.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
